<% @page_title = "Quizme! - Courses" %>

<div class="pagehead">
    <div class="innerHeadContainer">
        <h1>Courses</h1>
    </div>
</div>


<div class="content">
    <div id="listCourses">
    <% @courses.each do |course| %>
    <div>
       <p><%= link_to course.course_name, course_path(course) %></p>
    </div>
    <% end %>
    </div>
    <p><%= link_to "Add New Course", new_course_path, :class => "button" %></p>
    

</div>


